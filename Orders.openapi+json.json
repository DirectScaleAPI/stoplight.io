            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "The shipping carrier and tracking number.",
          "content": {
            "application/json": {
              "example": {
                "Carrier": "UPS",
                "TrackingNumber": "ABC-123-456"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Package shipping status was successfully updated."
          },
          "404": {
            "description": "The package was not found."
          }
        }
      }
