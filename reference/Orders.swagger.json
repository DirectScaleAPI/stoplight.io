    "/packages/{packageId}/shipped": {
      "put": {
        "description": "Set the package status to shipped.  This method is used to notify the order processing system that the package was shipped by an external system.<br/><br/>",
        "operationId": "59e6191db3ac880abcc07e62",
        "summary": "Set Package Shipped",
        "parameters": [
          {
            "name": "packageId",
            "in": "path",
            "description": "Package Id",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "example": "{\n    \"Carrier\": \"UPS\",\n    \"TrackingNumber\": \"ABC-123-456\"\n}"
            },
            "description": "The shipping carrier and tracking number."
          }
        ],
        "consumes": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Package shipping status was successfully updated."
          },
          "404": {
            "description": "The package was not found."
          }
        },
        "tags": [
          "Orders"
        ]
      }
    },
