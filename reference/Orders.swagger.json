    "/external-orders": {
      "post": {
        "description": "Import an order that has been processed by an external system.  This API will import the order data for commission calculations only.  It is assumed that all processing, billing, and shipping of goods for the order has being done by an external system.<br/><br/>\n<b>NOTE;</b> This operation is only available on Disco version 10 or later.<br/>\n<br/>\n<b>PREVIEW:</b> Please note that this method is still under development.<br/>\n<br/>",
        "operationId": "import-external-order",
        "summary": "Import External Order",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "example": "{    \n    \"CustomerId\": 0,\n    \"OrderType\": 0,\n    \"ExternalOrderId\": \"string\",\n    \"OrderDate\": \"date\",\n    \"CommissionDate\": \"date\",\n    \"InvoiceDate\": \"date\",\n    \"PartyId\": 0,\n    \"ShippingAddress\": {\n        \"Street1\": \"string\",\n        \"Street2\": \"string\",\n        \"Street3\": \"string\",\n        \"City\": \"string\",\n        \"PostalCode\": \"string\",\n        \"Region\": \"string\",\n        \"CountryCode\": \"string\"\n    },\n    \"Phone\": \"string\",\n    \"SpecialInstructions\": \"string\",\n    \"CurrencyCode\": \"string\",\n    \"LineItems\": [\n        {\n            \"Sku\": \"string\",\n            \"ProductName\": \"string\",\n            \"Quantity\": 0.0,\n            \"Price\": 0.0\n        },\n        {\n            \"Sku\": \"string\",\n            \"ProductName\": \"string\",\n            \"Quantity\": 0.0,\n            \"Price\": 0.0\n        }\n    ],\n    \"SubTotal\": 0.0,\n    \"ShippingTotal\": 0.0,\n    \"TaxTotal\": 0.0,\n    \"Total\": 0.0,\n    \"TotalCv\": 0.0,\n    \"TotalQv\": 0.0,\n    \"TotalBonus\": 0.0    \n}"
            }
          }
        ],
        "consumes": [
          "application/json"
        ],
        "responses": {
          "201": {
            "description": "Order successfully imported."
          }
        },
        "tags": [
          "Orders"
        ]
      }
    },
